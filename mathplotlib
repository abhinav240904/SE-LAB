import matplotlib.pyplot as plt
import numpy as np

xpoints = np.array([1, 8])
ypoints = np.array([3, 10])

plt.plot(xpoints, ypoints)
plt.show()


#Program with axis lables and title commands 
import matplotlib.pyplot as plt
import numpy as np

# coordinates of the two points
xpoints = np.array([1, 8])
ypoints = np.array([2, 6])
plt.xlabel('X-axis')
plt.ylabel('Y-axis')
plt.title('Plot with Two Points')
plt.plot(xpoints, ypoints)
plt.show()
